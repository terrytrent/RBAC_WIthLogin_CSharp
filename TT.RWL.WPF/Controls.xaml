<Window x:Class="TT.RWL.WPF.Controls"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TT.RWL.WPF"
        mc:Ignorable="d"
        Title="Controls" Height="211" Width="307">
    <Grid>
        <Button x:Name="deleteSelectedButton" Content="Delete Selected" HorizontalAlignment="Left" Margin="206,140,0,0" VerticalAlignment="Top" Width="75" FontSize="9.333" Click="deleteSelectedButton_Click"/>
