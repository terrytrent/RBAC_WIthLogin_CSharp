        <Button x:Name="editSelectedButton" Content="Edit Selected" HorizontalAlignment="Left" Margin="206,78,0,0" VerticalAlignment="Top" Width="75" FontSize="9.333" Click="editSelectedButton_Click"/>
        <Button x:Name="createNewButton" Content="Create New" HorizontalAlignment="Left" Margin="206,109,0,0" VerticalAlignment="Top" Width="75" FontSize="9.333" Click="createNewButton_Click"/>
        <Grid Margin="26,0,93,10">
            <Label x:Name="Read" Content="Read: " HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="57" Margin="0,41,0,0"/>
            <Label x:Name="ReadResults" Content="ReadResults" HorizontalAlignment="Left" Margin="62,41,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="113" HorizontalContentAlignment="Right"/>
            <Label x:Name="Write" Content="Write:" HorizontalAlignment="Left" Margin="0,72,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="57"/>
            <Label x:Name="WriteResults" Content="WriteResults" HorizontalAlignment="Left" Margin="62,72,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="113" HorizontalContentAlignment="Right"/>
            <Label x:Name="Create" Content="Create: " HorizontalAlignment="Left" Margin="0,103,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="57"/>
            <Label x:Name="CreateResults" Content="CreateResults" HorizontalAlignment="Left" Margin="62,103,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="113" HorizontalContentAlignment="Right"/>
            <Label x:Name="Delete" Content="Delete:" HorizontalAlignment="Left" Margin="0,134,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="57"/>
            <Label x:Name="DeleteResults" Content="DeleteResults" HorizontalAlignment="Left" Margin="62,134,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.193,-0.244" Width="113" HorizontalContentAlignment="Right"/>
            <Label x:Name="enumLabel" Content="enum" HorizontalAlignment="Left" VerticalAlignment="Top" HorizontalContentAlignment="Right" RenderTransformOrigin="-0.193,-0.244" Width="103" Margin="62,10,0,0"/>
            <Label x:Name="usrename" Content="username" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top"/>
        </Grid>

    </Grid>
</Window>
